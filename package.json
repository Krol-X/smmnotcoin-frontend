{
  "name": "smmnotcoin-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --check src && eslint src",
    "format": "prettier --write **/*.{js,ts,json,svelte}",
